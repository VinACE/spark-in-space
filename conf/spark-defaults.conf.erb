spark.executor.extraClassPath=/app/spark-home/lib/hadoop-aws-shaded-no-jackson-0.1-SNAPSHOT-shaded.jar
spark.executor.memory=12g
spark.driver.memory=2g
spark.driver.extraClassPath=/app/spark-home/lib/hadoop-aws-shaded-no-jackson-0.1-SNAPSHOT-shaded.jar
spark.hadoop.fs.s3a.impl=org.apache.hadoop.fs.s3a.S3AFileSystem
spark.hadoop.fs.AbstractFileSystem.s3a.impl=org.apache.hadoop.fs.s3a.S3A
spark.hadoop.fs.defaultFS=s3a://<%= ENV["BUCKETEER_BUCKET_NAME"] %>
spark.hadoop.fs.default.name=s3a://<%= ENV["BUCKETEER_BUCKET_NAME"] %>
spark.hadoop.fs.s3a.awsAccessKeyId=<%= ENV["BUCKETEER_AWS_ACCESS_KEY_ID"] %>
spark.hadoop.fs.s3a.awsSecretAccessKey=<%= ENV["BUCKETEER_AWS_SECRET_ACCESS_KEY"] %>
spark.hadoop.fs.s3a.access.key=<%= ENV["BUCKETEER_AWS_ACCESS_KEY_ID"] %>
spark.hadoop.fs.s3a.secret.key=<%= ENV["BUCKETEER_AWS_SECRET_ACCESS_KEY"] %>
